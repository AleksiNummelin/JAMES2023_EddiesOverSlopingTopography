  DITFLX   = .false.
  DISFLX   = .false.
  SRXBAL   = .true.
  SCFILE   = '/cluster/shared/noresm/inputdata/ocn/blom/bndcon/sss_clim_core_tnx1v4_20170604.nc'
  SMTFRC   = .false.
  SPRFAC   = .true.
  ATM_PATH = 'unset'
  ITEST    = 60
